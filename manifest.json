{
  "manifest_version": 3,
  "name": "Взлом Сетевого города",
  "description": "Удаляет disable в input ",
  "version": "1.0",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["src/main.js"],
      "matches": [
        "https://www.google.com/*",
        "https://poo.tomedu.ru/*"
      ]
    }
  ]
}